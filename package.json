{
  "name": "anatomy-of-consensus",
  "version": "1.0.0",
  "scripts": {
    "dev": "next \"src/app/\"",
    "preserve": "npm run build-semantic && npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps && npm run install-deps",
    "serve": "NODE_ENV=production firebase serve",
    "predeploy": "npm run build-public && npm run build-funcs && npm run build-app && npm run copy-deps",
    "deploy": "firebase deploy",
    "clean": "rimraf \"dist/functions/**\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build-app": "next build \"src/app/\"",
    "copy-deps": "cpx \"*{package.json,package-lock.json,yarn.lock}\" \"dist/functions\" -C",
    "install-deps": "cd \"dist/functions\" && npm i",
    "build-semantic": "cd src/public/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/public/semantic && npm run build-semantic && gulp watch"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "firebase": "^5.4.2",
    "firebase-admin": "^5.12.1",
    "firebase-functions": "^2.0.5",
    "gulp": "^3.9.1",
    "next": "6.1.1",
    "next-routes": "^1.4.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-favicon": "0.0.14",
    "react-redux": "^5.0.1",
    "react-scroll-into-view": "^1.3.2",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.4.2",
    "redux-form-website-template": "0.0.123",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.1.0",
    "semantic-ui": "^2.3.3",
    "semantic-ui-react": "^0.82.3"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.55",
    "@babel/core": "7.0.0-beta.55",
    "@babel/runtime": "7.0.0-beta.55",
    "cpx": "^1.5.0",
    "firebase-tools": "3.18.4",
    "prettier": "1.12.1",
    "rimraf": "2.6.2",
    "semantic-ui": "^2.3.3"
  }
}
